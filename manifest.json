{
  "name": "CSV Viewer",
  "version": "0.2",
  "description": "Allows viewing csv files (.csv) directly inside Chrome.",
  "manifest_version": 2,
  "icons": {
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*", "file://*/*"
      ],
      "js": ["csvify.js"]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ]
}
